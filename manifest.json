{
  "description": "Creates a console log interface on the page",
  "manifest_version": 2,
  "name": "ConsoleLogElement",
  "version": "2.1.0",
  "homepage_url": "https://github.com/hchiam/console-log-element",
  "icons": {
    "48": "console-log-element.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["brain.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
